@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema <schema="http://schema.org/>
@prefix org: <http://www.w3.org/ns/org#> .

#################################################################
#
#    Classes
#
#################################################################

<http://data.gmdsp.org.uk/def/council/parking/ParkingSite>
      rdfs:label "Parking site"^^xsd:string ;
      rdfs:subClassOf schema:ParkingFacility ;
      rdfs:isDefinedBy <http://data.gmdsp.org.uk/def/council/parking> .

<http://data.gmdsp.org.uk/def/council/parking/ParkingSiteOperator>
      rdfs:label "Parking site operator"^^xsd:string ;
      rdfs:subClassOf org:Organization ;
      rdfs:isDefinedBy <http://data.gmdsp.org.uk/def/council/parking> .

#################################################################
#
#    Properties
#
#################################################################

<http://data.gmdsp.org.uk/def/council/parking/operator>
      rdf:type rdf:Property ;
      rdfs:label "Car park operator"^^xsd:string ;
	  rdfs:comment "Which organisation is responsible for operating a particular car park" ;
      rdfs:domain <http://data.gmdsp.org.uk/def/council/parking/ParkingSite> ;
      rdfs:range <http://data.gmdsp.org.uk/def/council/parking/ParkingSiteOperator> ;
      rdfs:isDefinedBy <http://data.gmdsp.org.uk/def/council/parking> .

<http://data.gmdsp.org.uk/def/council/parking/totalNumberOfSpaces>
      rdf:type rdf:Property ;
      rdfs:label "Total number of spaces"^^xsd:string ;
	rdfs:comment "The total number of parking spaces in a particular car park" ;
      rdfs:domain <http://data.gmdsp.org.uk/def/council/parking/ParkingSite> ;
      rdfs:range xsd:integer ;
      rdfs:isDefinedBy <http://data.gmdsp.org.uk/def/council/parking> .

<http://data.gmdsp.org.uk/def/council/parking/totalNumberOfDisabledSpaces>
      rdf:type rdf:Property ;
      rdfs:label "Total number of spaces"^^xsd:string ;
	rdfs:comment "The total number of disabled parking spaces in a particular car park" ;
      rdfs:domain <http://data.gmdsp.org.uk/def/council/parking/ParkingSite> ;
      rdfs:range xsd:integer ;
      rdfs:isDefinedBy <http://data.gmdsp.org.uk/def/council/parking> .

<http://data.gmdsp.org.uk/def/council/parking/totalNumberOfFreeSpaces>
      rdf:type rdf:Property ;
      rdfs:label "Total number of spaces"^^xsd:string ;
	rdfs:comment "The total number of free parking spaces in a particular car park" ;
      rdfs:domain <http://data.gmdsp.org.uk/def/council/parking/ParkingSite> ;
      rdfs:range xsd:integer ;
      rdfs:isDefinedBy <http://data.gmdsp.org.uk/def/council/parking> .

<http://data.gmdsp.org.uk/def/council/parking/totalNumberOfChargaableSpaces>
      rdf:type rdf:Property ;
      rdfs:label "Total number of spaces"^^xsd:string ;
	rdfs:comment "The total number of chargeable parking spaces in a particular car park" ;
      rdfs:domain <http://data.gmdsp.org.uk/def/council/parking/ParkingSite> ;
      rdfs:range xsd:integer ;
      rdfs:isDefinedBy <http://data.gmdsp.org.uk/def/council/parking> .

<http://data.gmdsp.org.uk/def/council/parking/totalNumberOfInfoPoints>
      rdf:type rdf:Property ;
      rdfs:label "Total number of spaces"^^xsd:string ;
	rdfs:comment "The total number of information points in a particular car park" ;
      rdfs:domain <http://data.gmdsp.org.uk/def/council/parking/ParkingSite> ;
      rdfs:range xsd:integer ;
      rdfs:isDefinedBy <http://data.gmdsp.org.uk/def/council/parking> .

<http://data.gmdsp.org.uk/def/council/parking/totalNumberOfMachines>
      rdf:type rdf:Property ;
      rdfs:label "Total number of spaces"^^xsd:string ;
	rdfs:comment "The total number of payment machines in a particular car park" ;
      rdfs:domain <http://data.gmdsp.org.uk/def/council/parking/ParkingSite> ;
      rdfs:range xsd:integer ;
      rdfs:isDefinedBy <http://data.gmdsp.org.uk/def/council/parking> .

<http://data.gmdsp.org.uk/def/council/parking/totalNumberOfCCTVCameras>
      rdf:type rdf:Property ;
      rdfs:label "Total number of spaces"^^xsd:string ;
      rdfs:comment "The total number of CCTV cameras in a particular car park" ;
      rdfs:domain <http://data.gmdsp.org.uk/def/council/parking/ParkingSite> ;
      rdfs:range xsd:integer ;
      rdfs:isDefinedBy <http://data.gmdsp.org.uk/def/council/parking> .

<http://data.gmdsp.org.uk/def/council/parking/electricVehicleChargingPoint>
      rdf:type rdf:Property ;
      rdfs:label "Electric vehicle charging point"^^xsd:string ;
      rdfs:comment "Does the car park have charging points for electric vehicles?" ;
      rdfs:domain <http://data.gmdsp.org.uk/def/council/parking/ParkingSite> ;
      rdfs:range xsd:string ;
      rdfs:isDefinedBy <http://data.gmdsp.org.uk/def/council/parking> .
      
<http://data.gmdsp.org.uk/def/council/parking/informationPage>
        a                   rdf:Property ;
        rdfs:comment        "Page (given by URL) with information for this parking site."^^xsd:string ;
        rdfs:domain         <http://data.gmdsp.org.uk/def/council/parking/ParkingSite> ;
        rdfs:label          "Information page"^^xsd:string ;
        rdfs:subPropertyOf  <http://xmlns.com/foaf/0.1/page> ;
        rdfs:isDefinedBy <http://data.gmdsp.org.uk/def/council/parking> .

<http://data.gmdsp.org.uk/def/council/parking/parkingType>
        a                   rdf:Property ;
        rdfs:comment        "What type of parking is the site eg. pay and display"^^xsd:string ;
        rdfs:domain         <http://data.gmdsp.org.uk/def/council/parking/ParkingSite> ;
        rdfs:label          "Parking type"^^xsd:string ;
        rdfs:subPropertyOf  <http://xmlns.com/foaf/0.1/page> ;
        rdfs:range <http://data.gmdsp.org.uk/council/parking/parking-type> ;
        rdfs:isDefinedBy <http://data.gmdsp.org.uk/def/council/parking> .
