        rdfs:domain   <http://data.gmdsp.org.uk/def/fire-and-rescue/FireStation> ;
        rdfs:label    "Call sign"^^xsd:string ;
        rdfs:range    xsd:string .


<http://data.gmdsp.org.uk/def/fire-and-rescue/incidentReference>
